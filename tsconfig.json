{
    "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
    },
    "include": [
        "src/**/*", 
        "src/types",
    ],
    "exclude": [
        "node_modules",
        "generated"
    ]
}  